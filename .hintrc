{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off",
        "autocomplete-valid": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "box-align",
          "text-align: -webkit-match-parent"
        ]
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "button-type": "off",
    "axe/structure": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 120",
    "not chrome <= 120",
    "not edge <= 120",
    "not opera <= 106",
    "not samsung <= 23"
  ]
}